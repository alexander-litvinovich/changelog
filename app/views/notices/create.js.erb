var newNotice = document.querySelector("#<%= dom_id @release, :new_notice %>");

newNotice.innerHTML = '<%=j render "shared/new-notice", release: @release %>'

newNotice.parentNode.insertBefore(createElementFromHTML('<%=j render "shared/notice", notice: @notice %>'), newNotice);



